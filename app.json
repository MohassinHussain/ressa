{
  "expo": {
    "name": "ressa",
    "slug": "ressa",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/new-logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/new-logo.png",
        "backgroundColor": "#000000",
        "borderRadius": 20
      },
      "package": "com.mohassin.ressa",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/new-logo.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/new-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000",
          "borderRadius": 20
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}
